package Practise;

import java.util.Scanner;

public class P02Odd {
	public static void printSum(int n) {
		int sum = 0;
		//int = n;
		
		for (int i = 0; i<=n; i++){
			if(i % 2 != 0) {
				sum = sum + i;
			}
		}
		System.out.println(sum);
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		printsum(n);
	}

}
